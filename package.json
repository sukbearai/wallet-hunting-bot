{
  "name": "telegram-bot-starter",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "prepare": "nitropack prepare",
    "dev": "nitro dev",
    "build": "nitro build",
    "preview": "sudo npx wrangler pages dev dist/",
    "lint": "eslint --cache .",
    "format": "prettier \"**/*.{css,html,json,md,ts,tsx,yml}\" --write",
    "typecheck": "nitropack prepare && tsc",
    "release": "bumpp --no-push && tsx scripts/deploy.ts",
    "test:types": "tsc --noEmit",
    "deploy": "pnpm run build && node .output/server/index.mjs"
  },
  "dependencies": {
    "agent-twitter-client": "^0.0.18",
    "consola": "^3.3.3",
    "date-fns": "^4.1.0",
    "js-sha1": "^0.7.0",
    "nitropack": "latest",
    "socks-proxy-agent": "^8.0.5",
    "telegraf": "^4.16.3",
    "undici": "^7.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "bumpp": "^9.9.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
